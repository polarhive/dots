{
  "promptDelete": false,
  "vimMode": false,
  "propertiesInDocument": "hidden",
  "trashOption": "local",
  "attachmentFolderPath": "./attachments",
  "userIgnoreFilters": [
    ".pdf"
  ],
  "showRibbon": false,
  "showInlineTitle": true,
  "alwaysUpdateLinks": true,
  "newFileLocation": "current",
  "newLinkFormat": "shortest",
